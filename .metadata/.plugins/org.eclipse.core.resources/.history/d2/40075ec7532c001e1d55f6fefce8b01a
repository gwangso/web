<div class="row">
	<div class="col">
		<h1>저장도서목록</h1>
		<div id="div_list"></div>
	</div>
</div>

<!-- handlebars -->
<script id="temp_list" type="text/x-handlebars-template">
	
</script>

<script>
	$.ajax({
		type:"get",
		url:"/book/list.json",
		dataType:"json",
		data: {page:},
		success:function(data){
			const com_temp_list = Handlebars.compile($("temp_list").val());
			const html_list = com_temp_list(data);
			$("#div_list").html(html_list);
		}
	})
</script>